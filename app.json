{
  "expo": {
    "name": "FamilyNookMobileApp",
    "slug": "FamilyNookMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mainlogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/mainlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.btimofeyev.familynook",
      "buildNumber": "1",
      "supportsTablet": true,
      "jsEngine": "hermes",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "image": "./assets/mainlogo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.btimofeyev.familynook",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO"
      ],
      "config": {
        "usesCleartextTraffic": true
      },
      "googleServicesFile": "./google-services.json",
      "jsEngine": "hermes",
      "splash": {
        "image": "./assets/mainlogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/mainlogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "024ae2e1-c689-4f1d-99c7-df5af4322a34"
      }
    },
    "owner": "btimofeyev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/024ae2e1-c689-4f1d-99c7-df5af4322a34"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/mainlogo.png",
          "color": "#000000",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "FamilyNook",
          "iosDisplayInForeground": true
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-router"
    ]
  }
}
